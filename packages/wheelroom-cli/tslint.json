{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-plugin-prettier",
    "tslint-config-prettier"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [true, "single", "jsx-double"],
    "interface-name": [false],
    "jsx-no-multiline-js": [false],
    "prettier": true
  },
  "rulesDirectory": []
}
