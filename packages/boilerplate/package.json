{
  "author": "Jacco Meijer",
  "bugs": {
    "url": "https://github.com/jaccomeijer/wheelroom/issues"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "13.2.0",
    "@contentful/rich-text-types": "13.1.0",
    "@emotion/core": "10.0.14",
    "@jacco-meijer/content-models": "^2.0.0-alpha.6",
    "@jacco-meijer/styled-system": "^2.0.0-alpha.5",
    "@jacco-meijer/wheelroom": "^2.0.0-alpha.4",
    "@jacco-meijer/wheelroom-plugin-contentful": "^1.0.0-alpha.4",
    "@jacco-meijer/wheelroom-plugin-templates": "^1.0.0-alpha.6",
    "camelcase": "5.3.1",
    "dotenv": "8.0.0",
    "emotion-theming": "10.0.14",
    "gatsby": "2.13.23",
    "gatsby-plugin-emotion": "4.1.2",
    "gatsby-plugin-offline": "2.2.4",
    "gatsby-plugin-react-helmet": "3.1.2",
    "gatsby-plugin-tslint": "0.0.2",
    "gatsby-plugin-typescript": "2.1.2",
    "gatsby-plugin-web-font-loader": "1.0.4",
    "gatsby-source-contentful": "2.1.13",
    "gatsby-source-filesystem": "2.1.5",
    "gatsby-theme-wheelroom": "^2.0.0-alpha.9",
    "gatsby-transformer-json": "2.2.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1",
    "yargs": "13.3.0"
  },
  "description": "Gatsby static site boilerplate with React and Typescript",
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/jest": "24.0.15",
    "@types/node": "12.6.3",
    "@types/react": "16.8.23",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.0.8",
    "@types/yargs": "13.0.0",
    "conventional-changelog-cli": "2.0.21",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "tslint": "5.18.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.0.0",
    "typescript": "3.5.3"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "homepage": "https://github.com/jaccomeijer/wheelroom#readme",
  "license": "MIT",
  "main": "index.js",
  "name": "boilerplate",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaccomeijer/wheelroom.git"
  },
  "scripts": {
    "build:clean": "rm -rf ./dist",
    "build:packages": "tsc --project src/packages/tsconfig.json",
    "build:load-env": "export $(grep -v '^#' .env | xargs)",
    "build:gatsby": "gatsby build",
    "build": "npm-run-all build:*",
    "develop": "gatsby develop",
    "lint": "tslint --project tsconfig.json",
    "netlify": "npm-run-all build:clean build:packages build:gatsby",
    "start": "npm run develop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "demo-version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "demo-postversion": "git push && git push --tags",
    "wheelroom": "wheelroom",
    "wr:create-models": "wheelroom create-models",
    "wr:create-content": "wheelroom create-content",
    "wr:create-fragments": "wheelroom create-files fragments src/wheelroom-fragments",
    "wr:create-components-from-config": "wheelroom create-files components src/components",
    "wr:create-components-base": "wheelroom create-files components-base src/components",
    "wr:create-new-config": "wheelroom create-files config src/packages/component-configs",
    "wr:delete-content": "wheelroom delete-content",
    "wr:list": "wheelroom list"
  },
  "version": "2.0.0-alpha.12"
}
