{
  "author": "Jacco Meijer",
  "bugs": {
    "url": "https://github.com/jaccomeijer/wheelroom/issues"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "13.2.0",
    "@contentful/rich-text-types": "13.1.0",
    "@emotion/core": "10.0.10",
    "@jacco-meijer/content-models": "^1.1.4",
    "@jacco-meijer/styled-system": "^1.1.0",
    "@jacco-meijer/wheelroom": "^1.7.1",
    "dotenv": "8.0.0",
    "emotion-theming": "10.0.10",
    "gatsby": "2.9.4",
    "gatsby-plugin-emotion": "4.0.7",
    "gatsby-plugin-offline": "2.1.3",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-tslint": "0.0.2",
    "gatsby-plugin-typescript": "2.0.15",
    "gatsby-plugin-web-font-loader": "1.0.4",
    "gatsby-source-contentful": "2.0.71",
    "gatsby-source-filesystem": "2.0.39",
    "gatsby-theme-wheelroom": "^1.2.3",
    "gatsby-transformer-json": "2.1.11",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-helmet": "5.2.1"
  },
  "description": "Gatsby static site boilerplate with React and Typescript",
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/jest": "24.0.15",
    "@types/node": "12.0.8",
    "@types/react": "16.8.20",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.0.8",
    "conventional-changelog-cli": "2.0.21",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.0.0",
    "typescript": "3.5.2"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "homepage": "https://github.com/jaccomeijer/wheelroom#readme",
  "license": "MIT",
  "main": "index.js",
  "name": "boilerplate",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaccomeijer/wheelroom.git"
  },
  "scripts": {
    "build:clean": "rm -rf ./dist",
    "build:packages": "tsc --project src/packages/tsconfig.json",
    "build:load-env": "export $(grep -v '^#' .env | xargs)",
    "build:gatsby": "gatsby build",
    "build": "npm-run-all build:*",
    "contentful:create-content": "wheelroom create-content",
    "contentful:create-models": "wheelroom create-models",
    "contentful:delete-content": "wheelroom delete-content",
    "develop": "gatsby develop",
    "create-files:fragments": "wheelroom create-fragments src/wheelroom-fragments",
    "create-files:new-model": "wheelroom create-new-model src/components src/packages/component-configs",
    "wheelroom": "wheelroom",
    "lint": "tslint --project tsconfig.json",
    "list": "wheelroom list",
    "netlify": "npm-run-all build:clean build:packages build:gatsby",
    "start": "npm run develop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "postversion": "git push && git push --tags"
  },
  "version": "1.6.1"
}
