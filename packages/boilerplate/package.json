  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "13.4.0",
    "@contentful/rich-text-types": "13.4.0",
    "@emotion/core": "10.0.22",
    "@jacco-meijer/content-models": "3.0.0",
    "@jacco-meijer/styled-system": "3.0.3",
    "@jacco-meijer/wheelroom": "2.1.1",
    "@jacco-meijer/wheelroom-plugin-boilerplate": "1.0.2",
    "@jacco-meijer/wheelroom-plugin-contentful": "1.1.2",
    "@jacco-meijer/wheelroom-plugin-graphql": "1.0.2",
    "camelcase": "5.3.1",
    "dotenv": "8.2.0",
    "emotion-theming": "10.0.19",
    "gatsby": "2.18.13",
    "gatsby-plugin-emotion": "4.1.18",
    "gatsby-plugin-offline": "3.0.29",
    "gatsby-plugin-react-helmet": "3.1.18",
    "gatsby-plugin-tslint": "0.0.2",
    "gatsby-plugin-typescript": "2.1.23",
    "gatsby-plugin-web-font-loader": "1.0.4",
    "gatsby-source-contentful": "2.1.71",
    "gatsby-source-filesystem": "2.1.42",
    "gatsby-theme-wheelroom": "2.0.2",
    "gatsby-transformer-json": "2.2.22",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "yargs": "15.0.2"
  },
  "description": "Gatsby static site boilerplate with React and Typescript",
  "devDependencies": {
    "@types/dotenv": "8.2.0",
    "@types/node": "12.12.19",
    "@types/react": "16.9.16",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "5.0.14",
    "@types/yargs": "13.0.3",
    "conventional-changelog-cli": "2.0.31",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
    "tslint": "5.20.1",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.1.0",
    "typescript": "3.7.3"
  },
  "engines": {
    "node": ">=13.2.0"
  },
  "homepage": "https://github.com/jaccomeijer/wheelroom#readme",
  "license": "MIT",
  "main": "index.js",
  "name": "boilerplate",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaccomeijer/wheelroom.git"
  },
  "scripts": {
    "build:clean": "rm -rf ./dist",
    "build:load-env": "export $(grep -v '^#' .env | xargs)",
    "build:gatsby": "gatsby build",
    "build": "npm-run-all build:*",
    "compile-config": "tsc --project src/config/tsconfig.json",
    "develop": "gatsby develop",
    "lint": "tslint --project tsconfig.json",
    "netlify": "npm-run-all build:clean build:packages build:gatsby",
    "start": "npm run develop",
    "test": "echo \"Error: no test specified\" && exit 1",
    "type-check": "tsc --noEmit",
    "demo-version": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "demo-postversion": "git push && git push --tags",
    "wr": "wheelroom",
    "wr:cm": "wheelroom create-models",
    "wr:cc": "wheelroom create-content",
    "wr:cf": "wheelroom create-files fragments src/wheelroom-fragments",
    "wr:new-components": "wheelroom create-files components src/components",
    "wr:components-base": "wheelroom create-files components-base src",
    "wr:new-config": "wheelroom create-files config src/packages/component-configs",
    "wr:dc": "wheelroom delete-content",
    "wr:ls": "wheelroom list"
  },
  "type": "commonjs",
  "version": "3.0.0"
}
