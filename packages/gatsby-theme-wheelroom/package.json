{
  "author": "Jacco Meijer <jacco@jaccomeijer.nl> (https://www.jaccomeijer.nl)",
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "bin": {
    "wheelroom": "bin/wheelroom.js"
  },
  "bugs": {
    "url": "https://github.com/jaccomeijer/wheelroom/issues"
  },
  "dependencies": {
    "@emotion/core": "10.0.10",
    "contentful-management": "5.8.0",
    "dotenv": "8.0.0",
    "emotion-theming": "10.0.10",
    "facepaint": "1.2.1",
    "fs-extra": "8.0.1",
    "gatsby": "2.9.4",
    "gatsby-plugin-emotion": "4.0.7",
    "gatsby-plugin-google-tagmanager": "2.0.15",
    "gatsby-plugin-offline": "2.1.3",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-tslint": "0.0.2",
    "gatsby-plugin-typescript": "2.0.15",
    "gatsby-plugin-web-font-loader": "1.0.4",
    "gatsby-source-contentful": "2.0.71",
    "gatsby-source-filesystem": "2.0.39",
    "gatsby-transformer-json": "2.1.11",
    "npm-run-all": "4.1.5",
    "react-app-polyfill": "1.0.1",
    "react-docgen-typescript": "1.12.4",
    "react-helmet": "5.2.1",
    "react-scripts": "3.0.1",
    "react-styleguidist": "9.1.6",
    "typescript": "3.5.2",
    "yargs": "13.2.4"
  },
  "description": "Site framework based on Contentful and Gatsby",
  "devDependencies": {
    "@types/dotenv": "6.1.1",
    "@types/facepaint": "1.2.0",
    "@types/fs-extra": "7.0.0",
    "@types/jest": "24.0.14",
    "@types/node": "12.0.8",
    "@types/react": "16.8.20",
    "@types/react-dom": "16.8.4",
    "@types/react-helmet": "5.0.8",
    "@types/yargs": "13.0.0",
    "contentful-cli": "0.27.0",
    "prettier": "1.18.2",
    "tslint": "5.17.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-loader": "3.5.4",
    "tslint-plugin-prettier": "2.0.1",
    "tslint-react": "4.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "files": [
    "assets/*",
    "bin/*",
    "components/*",
    "gatsby-config.*",
    "gatsby-node.*",
    "index.*",
    "lib/*"
  ],
  "homepage": "https://github.com/jaccomeijer/wheelroom#readme",
  "keywords": [
    "gatsby",
    "wheelroom"
  ],
  "license": "MIT",
  "main": "/index.js",
  "name": "gatsby-theme-wheelroom",
  "peerDependencies": {
    "react": "16.8.6",
    "react-dom": "16.8.6"
  },
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jaccomeijer/wheelroom.git"
  },
  "scripts": {
    "build": "npm-run-all build:*",
    "build:clean": "rm -rf ./lib ./components ./bin ./gatsby-config.* ./gatsby-node.* ./index.*",
    "build:lint": "tslint --project tsconfig.json",
    "build:compile": "tsc",
    "prepack": "npm run build",
    "prepublishOnly": "npm run build",
    "styleguide": "styleguidist server",
    "styleguide:build": "styleguidist build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "types": "/index.d.ts",
  "version": "0.1.5"
}
